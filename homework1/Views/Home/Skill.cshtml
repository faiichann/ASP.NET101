@{
    ViewData["Title"] = "Skill";
}
<div class="container-fluid" style="max-width: 1200px;">
    <h1>My @ViewData["Title"]</h1>
    <div class="card" style="box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); margin-top:50px;">
        <h5 class="card-header">Skill Level</h5>
        <div class="card-body">
            
            <h5 class="card-title">@ViewData["dev1"]</h5>
            <div class="progress">
                <div class="progress-bar progress-bar-striped " role="progressbar" style="width: 80%" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100">80%</div>
            </div>
            <h5 class="card-title">@ViewData["dev2"]</h5>
            <div class="progress">
                <div class="progress-bar progress-bar-striped " role="progressbar" style="width: 70%" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100">70%</div>
            </div>
            <h5 class="card-title">@ViewData["dev3"]</h5>
            <div class="progress">
                <div class="progress-bar progress-bar-striped " role="progressbar" style="width: 50%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">50%</div>
            </div>
            <h5 class="card-title">@ViewData["dev4"]</h5>
            <div class="progress">
                <div class="progress-bar progress-bar-striped " role="progressbar" style="width: 70%" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100">70%</div>
            </div>
            <h5 class="card-title">@ViewData["dev5"]</h5>
            <div class="progress">
                <div class="progress-bar progress-bar-striped " role="progressbar" style="width: 55%" aria-valuenow="55" aria-valuemin="0" aria-valuemax="100">55%</div>
            </div>
            <h5 class="card-title">@ViewData["dev6"]</h5>
            <div class="progress">
                <div class="progress-bar progress-bar-striped" role="progressbar" style="width: 40%" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100">40%</div>
            </div>
            <h5 class="card-title">@ViewData["dev7"]</h5>
            <div class="progress">
                <div class="progress-bar progress-bar-striped" role="progressbar" style="width: 20%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100">20%</div>
            </div>
        </div>
    </div>
</div>

