@{
    ViewData["Title"] = "About Me";
}
<div class="container-fluid" style="max-width: 1200px;">
    <div class="row">
        <div class="col-4">
            <div class="text-center">
                <img style="width: 600px; height: 500px; box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);" src="~/img/me.jpg" class="rounded img-fluid" />
            </div>
        </div>
        <div class="col-8" style="align-items: center; margin-top: 100px;">
            <blockquote class="blockquote text-center">
                <p class="mb-0 display-4">Hello,</p>
                <p class="mb-0 display-4">My name is faiichann.</p>
                <p class="mb-0 display-4"> I'm Software Developer</p>
                <footer class="blockquote-footer">@ViewData["Name"] <cite title="Source Title">@ViewData["Idnum"]</cite></footer>
            </blockquote>
        </div>
    </div>
</div>

<div class="container-fluid">
    <div class="card" style="box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); margin-top:50px;">
        <h5 class="card-header ">Personal Information</h5>
        <div class="card-body">
            <h5 class="card-title">@ViewData["Namenum"]</h5>
            <ul class="list-unstyled">
                <li class="media">
                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-gift-fill mr-3" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" d="M3 2.5a2.5 2.5 0 0 1 5 0 2.5 2.5 0 0 1 5 0v.006c0 .07 0 .27-.038.494H15a1 1 0 0 1 1 1v1a1 1 0 0 1-1 1H1a1 1 0 0 1-1-1V4a1 1 0 0 1 1-1h2.038A2.968 2.968 0 0 1 3 2.506V2.5zm1.068.5H7v-.5a1.5 1.5 0 1 0-3 0c0 .085.002.274.045.43a.522.522 0 0 0 .023.07zM9 3h2.932a.56.56 0 0 0 .023-.07c.043-.156.045-.345.045-.43a1.5 1.5 0 0 0-3 0V3z" />
                        <path d="M15 7v7.5a1.5 1.5 0 0 1-1.5 1.5H9V7h6zM2.5 16A1.5 1.5 0 0 1 1 14.5V7h6v9H2.5z" />
                    </svg>
                    <div class="media-body">
                        <h5 class="mt-0 mb-1">@ViewData["h1"]</h5>
                        <h6 class="card-text"> @ViewData["Birth"]</h6>
                    </div>
                </li>
                <li class="media my-4">
                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-house-door-fill mr-3" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path d="M6.5 10.995V14.5a.5.5 0 0 1-.5.5H2a.5.5 0 0 1-.5-.5v-7a.5.5 0 0 1 .146-.354l6-6a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 .146.354v7a.5.5 0 0 1-.5.5h-4a.5.5 0 0 1-.5-.5V11c0-.25-.25-.5-.5-.5H7c-.25 0-.5.25-.5.495z" />
                        <path fill-rule="evenodd" d="M13 2.5V6l-2-2V2.5a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5z" />
                    </svg>
                    <div class="media-body">
                        <h5 class="mt-0 mb-1">@ViewData["h2"]</h5>
                        <h6 class="card-text"> @ViewData["Home"]</h6>
                    </div>
                </li>
                <li class="media">
                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-pencil-square mr-3" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path d="M15.502 1.94a.5.5 0 0 1 0 .706L14.459 3.69l-2-2L13.502.646a.5.5 0 0 1 .707 0l1.293 1.293zm-1.75 2.456l-2-2L4.939 9.21a.5.5 0 0 0-.121.196l-.805 2.414a.25.25 0 0 0 .316.316l2.414-.805a.5.5 0 0 0 .196-.12l6.813-6.814z" />
                        <path fill-rule="evenodd" d="M1 13.5A1.5 1.5 0 0 0 2.5 15h11a1.5 1.5 0 0 0 1.5-1.5v-6a.5.5 0 0 0-1 0v6a.5.5 0 0 1-.5.5h-11a.5.5 0 0 1-.5-.5v-11a.5.5 0 0 1 .5-.5H9a.5.5 0 0 0 0-1H2.5A1.5 1.5 0 0 0 1 2.5v11z" />
                    </svg>
                    <div class="media-body">
                        <h5 class="mt-0 mb-1">@ViewData["h3"]</h5>
                        <h6 class="card-text"> @ViewData["Edu"]</h6>
                    </div>
                </li>
            </ul>
            <ul class="list-unstyled">
                <li class="media">
                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-briefcase-fill mr-3" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" d="M0 12.5A1.5 1.5 0 0 0 1.5 14h13a1.5 1.5 0 0 0 1.5-1.5V6.85L8.129 8.947a.5.5 0 0 1-.258 0L0 6.85v5.65z" />
                        <path fill-rule="evenodd" d="M0 4.5A1.5 1.5 0 0 1 1.5 3h13A1.5 1.5 0 0 1 16 4.5v1.384l-7.614 2.03a1.5 1.5 0 0 1-.772 0L0 5.884V4.5zm5-2A1.5 1.5 0 0 1 6.5 1h3A1.5 1.5 0 0 1 11 2.5V3h-1v-.5a.5.5 0 0 0-.5-.5h-3a.5.5 0 0 0-.5.5V3H5v-.5z" />
                    </svg>
                    <div class="media-body">
                        <h5 class="mt-0 mb-1">@ViewData["h4"]</h5>
                        <h6 class="card-text"> @ViewData["work1"] / @ViewData["work2"] /@ViewData["work3"]/ @ViewData["work4"]</h6>
                    </div>
                </li>
            </ul>
        </div>
    </div>
</div>